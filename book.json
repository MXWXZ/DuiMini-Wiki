{
    "language": "zh-hans",
    "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "github",
        "splitter",
        "expandable-chapters-small",
        "edit-link",
        "copy-code-button",
        "versions-select"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "disqus": {
            "shortName": "DuiMini-wiki"
        },
        "github": {
            "url": "https://github.com/MXWXZ/DuiMini"
        },
        "edit-link": {
            "base": "https://github.com/MXWXZ/DuiMini-wiki/blob/master/",
            "label": "编辑本页"
        },
        "versions": {
            "gitbookConfigURL": "https://raw.githubusercontent.com/MXWXZ/DuiMini-wiki/master/book.json",
            "options": [
                {
                    "value": "https://mxwxz.gitbooks.io/duimini-wiki/",
                    "text": "Latest"
                }
            ]
        }
    }
}
